function sh(strdate)
{
  	for(i=0 ; i<photos.photos.length ; i++)
	 {
 	      if(ChangeDateFormat(photos.photos[i].time.toString()) == strdate)
	      {
            outpic(i);
	       }
     }
	 document.getElementById("tdate").innerHTML=strdate;
	 
};
