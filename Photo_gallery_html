 //--------------------------------------resizing----------------------------
 function resizing(img) {
    var width = img.width;
    var height = img.height;

    var maxWidth = 160;
    var maxHeight = 160;
    var resizeWidth;
    var resizeHeight;
    
    var basisRatio = maxHeight / maxWidth;
    var imgRatio = height / width;

    if (imgRatio > basisRatio) {
        
        if (height > maxHeight) {
            resizeHeight = maxHeight;
            resizeWidth = Math.round((width * resizeHeight) / height);
        } else {
            resizeWidth = width;
            resizeHeight = height;
        }
        
    } else if (imgRatio < basisRatio) {
        
        if (width > maxWidth) {
            resizeWidth = maxWidth;
            resizeHeight = Math.round((height * resizeWidth) / width);
        } else {
            resizeWidth = width;
            resizeHeight = height;
        }
        
    } else {
        resizeWidth = width;
        resizeHeight = height;
    }
    
    img.width = resizeWidth;
    img.height = resizeHeight;}
//--------------------------------------resizing----------------------------
  
  
 //-------------------json date -> date----------------------------
 function ChangeDateFormat(jsondate) {
 jsondate = jsondate.replace("/Date(", "").replace(")/", "");
 if (jsondate.indexOf("+") > 0) {
 jsondate = jsondate.substring(0, jsondate.indexOf("+"));
 } else if (jsondate.indexOf("-") > 0) {
 jsondate = jsondate.substring(0, jsondate.indexOf("-"));
 }
 var date = new Date(parseInt(jsondate, 10)); 
 var month = date.getMonth() + 1 < 10 ? "0" + (date.getMonth() + 1) : date.getMonth() + 1;
 var currentDate = date.getDate() < 10 ? "0" + date.getDate() : date.getDate();
 return date.getFullYear() + "-" + month + "-" + currentDate ;
 }
 var strdate = ChangeDateFormat(photos.photos[0].time.toString());
//-------------------json date -> date----------------------------


//-------------------pictures output----------------------------
var count=0;
function outpic(i)
 {
 if (count < 5)
 {
   var img = document.createElement("img");
   img.src = photos.photos[i].imageURL;
   var ps = document.getElementById("dimg1");
   document.body.insertBefore(img,ps);
   img.onload = resizing(img);
   count++;
 }
 
  else if ( count >4 &&count < 10)
 {
   var img = document.createElement("img");
   img.src = photos.photos[i].imageURL;
   var ps = document.getElementById("dimg2");
   document.body.insertBefore(img,ps);
   img.onload = resizing(img);
   count++;
 }
 
  else if (count >9 &&count < 15)
 {
   var img = document.createElement("img");
   img.src = photos.photos[i].imageURL;
   var ps = document.getElementById("dimg3");
   document.body.insertBefore(img,ps);
   img.onload = resizing(img);
   count++;
 }
 
  else if (count >14 &&count < 20)
 {
   var img = document.createElement("img");
   img.src = photos.photos[i].imageURL;
   var ps = document.getElementById("dimg4");
   document.body.insertBefore(img,ps);
   img.onload = resizing(img);
   count++;
 }
 
  else if (count >19 &&count < 25)
 {
   var img = document.createElement("img");
   img.src = photos.photos[i].imageURL;
   var ps = document.getElementById("dimg5");
   document.body.insertBefore(img,ps);
   img.onload = resizing(img);
   count++;
 }
 
 };
